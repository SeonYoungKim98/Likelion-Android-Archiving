<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".MainActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_camera"
                android:id="@+id/camera"
        />
        <ImageButton
                android:layout_width="302dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/logo"
                android:id="@+id/instagram_logo"
                android:layout_marginBottom="8dp"/>
        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_send"
                android:id="@+id/direct_message"
        />
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/Story_bar">


            <LinearLayout
                    android:orientation="vertical"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:id="@+id/MyStory">

                <ImageButton
                        app:srcCompat="@drawable/pro_3"
                        android:id="@+id/myStory_pic"
                        android:layout_weight="1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"/>
                <TextView
                        android:text="나의스토리"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/mystory_name"
                        android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:id="@+id/Story_Test1">

                <ImageButton
                        app:srcCompat="@drawable/pro_3"
                        android:id="@+id/story_test_pic1"
                        android:layout_weight="1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"/>
                <TextView
                        android:text="test1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/story_test_name1"
                        android:layout_weight="1"/>
            </LinearLayout>


            <LinearLayout
                    android:orientation="vertical"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:id="@+id/Story_test2">
                <ImageButton
                        android:layout_height="50dp"
                        app:srcCompat="@drawable/pro_3"
                        android:id="@+id/story_test_pic2"
                        android:layout_weight="1"
                        android:layout_width="50dp"/>
                <TextView
                        android:text="test2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/stroy_test_name2"
                        android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1" android:id="@+id/Story_test3">
                <ImageButton
                        android:layout_height="50dp" app:srcCompat="@drawable/pro_3"
                        android:id="@+id/story_test_pic3"
                        android:layout_weight="1" android:layout_width="50dp"/>
                <TextView
                        android:text="test3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/stroy_test_name3"
                        android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:layout_weight="1" android:id="@+id/Story_test4">
                <ImageButton
                        android:layout_height="50dp" app:srcCompat="@drawable/pro_3"
                        android:id="@+id/story_test_pic4"
                        android:layout_weight="1" android:layout_width="50dp"/>
                <TextView
                        android:text="test4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/stroy_test_name4"
                        android:layout_weight="1"/>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

    <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler_View"
            android:layout_width="match_parent"
            android:layout_height="420dp"
            tools:listitem="@layout/item_feed"
    >
    </android.support.v7.widget.RecyclerView>


    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/navi">

        <ImageButton
                android:layout_width="76dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/home"
                android:id="@+id/go_home"
        />
        <ImageButton
                android:layout_width="78dp"
                android:layout_height="match_parent"
                app:srcCompat="@android:drawable/ic_menu_search"
                android:id="@+id/find"/>
        <ImageButton
                android:layout_width="89dp"
                android:layout_height="match_parent"
                app:srcCompat="@android:drawable/ic_menu_add"
                android:id="@+id/upload"/>
        <ImageButton
                android:layout_width="82dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/heart"
                android:id="@+id/bu1"
        />
        <ImageButton
                android:layout_width="90dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/pro_3"
                android:id="@+id/myProfile"
        />

        />


    </LinearLayout>


</LinearLayout>